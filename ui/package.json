{
  "name": "styledoc-ui",
  "version": "1.0.0",
  "description": "styledoc default frontend",
  "main": "index.js",
  "scripts": {
    "start": "budo debug/debug.js:styledoc.js --live",
    "build": "browserify index.js | uglifyjs -c -m > styledoc.js"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "keywords": [
    "styledoc",
    "css",
    "documentation"
  ],
  "author": "Tom MacWright",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "budo": "^9.2.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {
    "highlight.js": "^9.8.0",
    "lowlight": "^1.5.0",
    "react-lowlight": "^1.0.2",
    "remark": "^6.2.0",
    "remark-react": "^3.1.0"
  }
}
